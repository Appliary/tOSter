{
  "name": "toster",
  "version": "22.607.2013",
  "description": "Operating System for your PROTOGEN",
  "type": "module",
  "author": "MicCarr <miccarr@me.com>",
  "license": "copyright",
  "scripts": {
    "start": "nodemon backend/index.js",
    "debug": "LOGLEVEL=silly npm start",
    "deploy": "node scripts/deploy.js",
    "prod": "node backend/index.js",
    "front": "cd frontend;npm i && npm start;cd ..",
    "front:build": "cd frontend;npm i && npm run build;cd ..",
    "front:test": "cd frontend;npm i && npm run test;cd .."
  },
  "dependencies": {
    "basic-auth": "2.0.1",
    "camo": "0.12.4",
    "chalk": "5.0.1",
    "dotstar": "1.0.0",
    "express": "4.18.1",
    "niceware": "3.0.0",
    "pi-spi": "1.2.1",
    "wifiscanner": "1.0.1",
    "wpasupplicant-manager": "0.1.0"
  },
  "devDependencies": {
    "nodemon": "2.0.16"
  },
  "nodemonConfig": {
    "verbose": false,
    "quiet": true,
    "ext": "js, cjs, mjs, json",
    "watch": [
      "backend/"
    ]
  },
  "imports": {
    "#Middlewares/*": "./backend/Middlewares/*.js",
    "#Models/*": "./backend/Models/*.js",
    "#Controllers/*": "./backend/Controllers/*.js",
    "#Routes/*": "./backend/Routes/*.js",
    "#Utils/*": "./backend/Utils/*.js"
  }
}
